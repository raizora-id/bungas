#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ridho/Documents/go/github.com/red-ant-colony/web-monorepo/node_modules/.pnpm/vite@6.3.3_@types+node@22.15.3_jiti@2.4.2_lightningcss@1.29.2_terser@5.39.0_tsx@4.19.3/node_modules/vite/bin/node_modules:/Users/ridho/Documents/go/github.com/red-ant-colony/web-monorepo/node_modules/.pnpm/vite@6.3.3_@types+node@22.15.3_jiti@2.4.2_lightningcss@1.29.2_terser@5.39.0_tsx@4.19.3/node_modules/vite/node_modules:/Users/ridho/Documents/go/github.com/red-ant-colony/web-monorepo/node_modules/.pnpm/vite@6.3.3_@types+node@22.15.3_jiti@2.4.2_lightningcss@1.29.2_terser@5.39.0_tsx@4.19.3/node_modules:/Users/ridho/Documents/go/github.com/red-ant-colony/web-monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ridho/Documents/go/github.com/red-ant-colony/web-monorepo/node_modules/.pnpm/vite@6.3.3_@types+node@22.15.3_jiti@2.4.2_lightningcss@1.29.2_terser@5.39.0_tsx@4.19.3/node_modules/vite/bin/node_modules:/Users/ridho/Documents/go/github.com/red-ant-colony/web-monorepo/node_modules/.pnpm/vite@6.3.3_@types+node@22.15.3_jiti@2.4.2_lightningcss@1.29.2_terser@5.39.0_tsx@4.19.3/node_modules/vite/node_modules:/Users/ridho/Documents/go/github.com/red-ant-colony/web-monorepo/node_modules/.pnpm/vite@6.3.3_@types+node@22.15.3_jiti@2.4.2_lightningcss@1.29.2_terser@5.39.0_tsx@4.19.3/node_modules:/Users/ridho/Documents/go/github.com/red-ant-colony/web-monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
